<% _.each(data.results, function(item) { %>
  <li>
    <figure class="artist_tweets_list_cover">
      <img src="<%= item.profile_image_url %>" alt="<%= item.from_usertitle %>">
      <figcaption class="artist_tweets_list_title">
        <strong>@<%= item.from_user %></strong><br>
        <%= item.text %>
      </figcaption>
    </figure>
  </li>
<% }) %>